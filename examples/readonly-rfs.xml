<ns0:RootFileSystem xmlns:ns0="https://www.linutronix.de/projects/Elbe" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" created="2009-05-20T08:50:56" revision="6" xsi:schemaLocation="https://www.linutronix.de/projects/Elbe dbsfed.xsd">
	<initvm>
		<buildtype>amd64</buildtype>
		<mirror>
			<primary_host>ftp.de.debian.org</primary_host>
			<primary_path>/debian</primary_path>
			<primary_proto>http</primary_proto>
			<url-list>
				<url>
					<binary>
						http://debian.linutronix.de/elbe-testing wheezy main
					</binary>
				</url>
			</url-list>
		</mirror>
		<noauth/>
		<suite>wheezy</suite>
		<portforwarding>
			<forward>
				<proto>tcp</proto>
				<buildenv>8080</buildenv>
				<host>8080</host>
			</forward>
		</portforwarding>
	</initvm>
	<project>
		<name>read-only-rfs</name>
		<version>1.0</version>
		<description>
			read-only rfs with elbe-updated
		</description>
		<buildtype>amd64</buildtype>
		<mirror>
			<primary_host>ftp.de.debian.org</primary_host>
			<primary_path>/debian</primary_path>
			<primary_proto>http</primary_proto>
			<url-list>
				<url>
					<binary>http://debian.linutronix.de/elbe-testing wheezy main</binary>
				</url>
			</url-list>
		</mirror>
		<noauth />
		<suite>wheezy</suite>
	</project>
	<target>
		<hostname>elbe-ro</hostname>
		<domain>tec.linutronix.de</domain>
		<passwd>foo</passwd>
		<console>ttyS0,115200</console>
		<package>
			<tar>
				<name>rorootfs.tar.gz</name>
			</tar>
		</package>
		<images>
		<msdoshd>
			<name>hd.img</name>
			<size>4G</size>
			<grub-install/>
			<partition>
				<size>100M</size>
				<label>boot</label>
			</partition>
			<partition>
				<size>remain</size>
				<label>rfs</label>
			</partition>
		</msdoshd>
		</images>
		<fstab>
			<bylabel>
				<label>rfs</label>
				<mountpoint>/</mountpoint>
				<fs><type>ext4</type></fs>
				<options>ro</options>
			</bylabel>
			<bylabel>
				<label>boot</label>
				<mountpoint>/boot</mountpoint>
				<fs><type>vfat</type></fs>
				<options>ro</options>
			</bylabel>
			<bydev>
				<source>sysfs</source>
				<mountpoint>/sys</mountpoint>
				<fs>
					<type>sysfs</type>
				</fs>
				<options>rw,nosuid,nodev,noexec,relatime</options>
			</bydev>
			<bydev>
				<source>devpts</source>
				<mountpoint>/dev/pts</mountpoint>
				<fs>
					<type>devpts</type>
				</fs>
				<options>rw,nosuid,noexec,relatime,gid=5,mode=620</options>
			</bydev>
			<bydev>
				<source>tmpfs</source>
				<mountpoint>/tmp</mountpoint>
				<fs>
					<type>tmpfs</type>
				</fs>
				<options>size=16M</options>
			</bydev>
		</fstab>
		<norecommend />
		<finetuning>
			<adduser passwd="foo" shell="/bin/bash">user</adduser>
			<mv path="/var">/var_debian</mv>
			<mkdir>/var</mkdir>
			<rm>/etc/mtab</rm>
			<rm>/run</rm>
			<command>ln -s /var/run /run</command>
			<rm>/etc/udev/rules.d/70-persistent-net.rules</rm>
			<rm>/lib/udev/rules.d/75-persistent-net-generator.rules</rm>
			<rm>/etc/nologin</rm>
			<rm>/etc/init.d/bootmisc.sh</rm>
			<rm>/etc/init.d/mountkernfs.sh</rm>
			<rm>/etc/init.d/mountdevsubfs.sh</rm>
			<rm>/etc/init.d/procps</rm>
			<rm>/etc/init.d/checkroot.sh</rm>
			<rm>/etc/init.d/kmod</rm>
			<rm>/etc/init.d/mtab.sh</rm>
			<rm>/etc/init.d/urandom</rm>
			<rm>/etc/init.d/checkfs.sh</rm>
			<rm>/etc/init.d/udev-mtab</rm>
			<rm>/etc/init.d/checkroot-bootclean.sh</rm>
			<rm>/etc/init.d/dbus</rm>
			<rm>/etc/init.d/motd</rm>
			<rm>/etc/init.d/mountall.sh</rm>
			<rm>/etc/init.d/mountnfs.sh</rm>
			<rm>/etc/init.d/mountall-bootclean.sh</rm>
			<rm>/etc/init.d/mountnfs-bootclean.sh</rm>
			<command>insserv -f -d -p /etc/init.d</command>
			<command>insserv -d mountfstab.sh</command>
			<command>insserv -d rofshooks.sh</command>
			<command>insserv -d elbe-updated</command>
		</finetuning>
		<pkg-list>
			<pkg>grub-pc</pkg>
			<pkg>linux-image-amd64</pkg>
			<pkg>elbe-updated</pkg>
			<pkg>openssh-client</pkg>
			<pkg>openssh-server</pkg>
			<pkg>bash</pkg>
			<pkg>gdbserver</pkg>
			<pkg>psmisc</pkg>
			<pkg>strace</pkg>
			<pkg>console-data</pkg>
			<pkg>kbd</pkg>
		</pkg-list>
	</target>
